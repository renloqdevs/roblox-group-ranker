{
  "name": "roblox-ranking-bot",
  "version": "1.2.0",
  "description": "A self-hosted Roblox group ranking bot with API endpoints and console UI",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "console": "node console/app.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:stop": "pm2 stop rankbot-api",
    "pm2:restart": "pm2 restart rankbot-api",
    "pm2:logs": "pm2 logs rankbot-api",
    "pm2:monit": "pm2 monit"
  },
  "keywords": [
    "roblox",
    "ranking",
    "bot",
    "group",
    "api",
    "console"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "noblox.js": "^4.15.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "pm2": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
